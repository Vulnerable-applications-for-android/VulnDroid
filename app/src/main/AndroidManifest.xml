<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.Mihir.VulnDroid.localstorage">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/title"
        android:theme="@style/Theme.AppCompat.Light.DarkActionBar">
        <activity android:name="com.Mihir.VulnDroid.localstorage.InternalStorage">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.Mihir.VulnDroid.localstorage.MainActivity" />
        </activity>
        <activity android:name="com.Mihir.VulnDroid.localstorage.ExternalStorage" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.Mihir.VulnDroid.localstorage.MainActivity" />
        </activity>
        <activity android:name="com.Mihir.VulnDroid.localstorage.insecureCert" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.Mihir.VulnDroid.localstorage.MainActivity" />
        </activity>
        <activity android:name="com.Mihir.VulnDroid.localstorage.ReadDatabase">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.Mihir.VulnDroid.localstorage.MainActivity" />
        </activity>
        <activity android:name="com.Mihir.VulnDroid.localstorage.Scoreboard">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.Mihir.VulnDroid.localstorage.MainActivity" />
        </activity>
        <activity android:name="com.Mihir.VulnDroid.localstorage.congo" />
        <activity android:name="com.Mihir.VulnDroid.localstorage.ReadString">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.Mihir.VulnDroid.localstorage.MainActivity" />
        </activity>
        <activity android:name="com.Mihir.VulnDroid.localstorage.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.Mihir.VulnDroid.localstorage.MainActivity" />
        <activity android:name="com.Mihir.VulnDroid.localstorage.Unhash_me">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.Mihir.VulnDroid.localstorage.MainActivity" />
        </activity>
        <activity android:name="com.Mihir.VulnDroid.localstorage.SharedPref">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.Mihir.VulnDroid.localstorage.MainActivity" />
        </activity>
        <activity android:name="com.Mihir.VulnDroid.localstorage.ReadMem">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.Mihir.VulnDroid.localstorage.MainActivity" />
        </activity>
        <activity android:name="com.Mihir.VulnDroid.localstorage.LogMe">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.Mihir.VulnDroid.localstorage.MainActivity" />
        </activity>
    </application>

</manifest>